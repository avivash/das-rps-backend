// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Game {
  uid                   String @id
  commit                String @unique
  player1               String
  player2               String
  player1Choice         Int
  player2Choice         Int
  player1RevealDeadline Int
  player2RevealDeadline Int
  status                Int
}
